<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel>
		<g:Label>Paste information below for 1 person</g:Label>
		<g:HorizontalPanel spacing="5">
			<g:RadioButton ui:field="emailChoice" name="submissionType">Email</g:RadioButton>
			<g:RadioButton ui:field="jsonChoice" name="submissionType">JSON</g:RadioButton>
		</g:HorizontalPanel>
		<g:TextArea width="400px" height="300px" ui:field="content"/>
		<g:HorizontalPanel spacing="2">
			<g:Button ui:field="importButton">Import</g:Button>
			<g:Button ui:field="cancelButton">Cancel</g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 