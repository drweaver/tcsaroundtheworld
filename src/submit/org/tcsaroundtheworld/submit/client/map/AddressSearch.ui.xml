<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel spacing="5">
		<g:HorizontalPanel spacing="3">
			<g:TextBox ui:field="addressQuery" />
			<g:Button ui:field="addressSearch">Find Address</g:Button>
		</g:HorizontalPanel>
		<g:SimplePanel ui:field="mapContainer" />
		<g:HorizontalPanel spacing="3">
			<g:Button ui:field="ok" enabled="false">OK</g:Button>
			<g:Button ui:field="cancel">Cancel</g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 